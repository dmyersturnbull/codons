journal = {Molecular biology and evolution},
keywords = {Animals,Codon,Drosophila melanogaster,Drosophila melanogaster: genetics,Escherichia coli,Escherichia coli: genetics,Evolution, Molecular,Mice,Protein Biosynthesis,Protein Conformation,Protein Folding,Proteins,Proteins: chemistry,Proteins: genetics,Proteins: metabolism,Saccharomyces cerevisiae,Saccharomyces cerevisiae: genetics,Selection, Genetic},
