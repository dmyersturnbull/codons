pmid = {18662548},
title = {{Mistranslation-induced protein misfolding as a dominant constraint on coding-sequence evolution.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2696314\&tool=pmcentrez\&rendertype=abstract},
volume = {134},
year = {2008}
}
