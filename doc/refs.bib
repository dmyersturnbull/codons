month = oct,
number = {10},
pages = {1973--83},
pmid = {8897597},
title = {{Protein secondary structural types are differentially coded on messenger RNA.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2143259\&tool=pmcentrez\&rendertype=abstract},
volume = {5},
